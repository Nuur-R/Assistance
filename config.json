{
    "model": "models/gemini-2.0-flash-exp",
    "generation_config": {
      "response_modalities": ["AUDIO"],
      "temperature": 0.7,
      "max_tokens": 100,
      "top_p": 0.9,
      "stop_sequences": ["\n", "."]
    },
    "audio_config": {
      "sample_rate": 16000,
      "chunk_size": 1024,
      "format": "pyaudio.paInt16"
    },
    "video_config": {
      "resolution": "1024x768",
      "frame_rate": 30,
      "format": "JPEG"
    },
    "api_config": {
      "timeout": 30,
      "retry_policy": {
        "max_retries": 3,
        "backoff_factor": 2
      },
      "api_version": "v1alpha"
    },
    "app_config": {
      "debug_mode": true,
      "log_level": "DEBUG",
      "max_queue_size": 10
    },
    "security_config": {
      "api_key": "your_api_key_here",
      "encryption_key": "your_encryption_key_here"
    },
    "localization_config": {
      "language": "id",
      "timezone": "UTC"
    },
    "resource_config": {
      "model_path": "/path/to/model",
      "data_path": "/path/to/data"
    },
    "custom_config": {
      "feature_flags": {
        "enable_audio_processing": true,
        "enable_video_processing": false
      },
      "user_preferences": {
        "theme": "dark",
        "notifications": true
      }
    }
  }